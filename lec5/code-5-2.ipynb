    "# -*- coding: utf-8 -*-\n",
    "from numpy import array, linalg\n",
    "A = array([[ 2.0,  0.0,  4.0],\n",
    "           [ 2.0,  1.0,  0.0],\n",
    "           [ 1.0, -1.0, -1.0]])\n",
    "b = array([1.0, 1.0, 0.0])\n",
    "x = linalg.solve(A,b) #线性方程求解函数（LU）\n",
    "print(\"x=\",x)\n"
   ]
